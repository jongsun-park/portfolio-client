{
  "name": "storybook-react",
  "version": "0.1.0",
  "engines": {
    "node": "12.19.0"
  },
  "private": true,
  "dependencies": {
    "node-sass": "4.14",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-scripts": "4.0.1",
    "redux": "^4.0.5",
    "styled-components": "^5.2.1"
  },
  "scripts": {
    "start": "react-scripts build",
    "pre:start": "npm run build:storybook",
    "start:dev": "react-scripts start",
    "start:storybook": "start-storybook -p 6006 -s public",
    "build:storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
